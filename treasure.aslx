          MakeExitVisible (Small alley entrance)
          MoveObject (Thief, Small alley)
          msg ("Thief stops at the middle of the Seaport street and enters a small alley between two buildings.")
          msg ("You can enter the " + CommandLink("go small alley entrance", "small alley") + " now.")
        }
        else if (this.parent = GetObject("Small alley")) {
          MoveObject (Thief, Thief home)
          msg ("Thief enters the house at the end of the alley.")
          UnlockExit (Thief house door)
          msg ("You can investigate this " +  CommandLink("go house", "house") + " now")
        }
        else if (this.parent = GetObject("Thief home")) {
          Ask ("Talk to Johny?") {
            if (result) {
              msg (CommandLink("ask Johny about the lost bag", "Hey John. I want the bag you stole from the square, and I'm going to return it. You are still young. You can do a lot more things if you stop stealing."))
            }
          }
        }
        else {
          random_move (this)
        }
      </look>
      <ask type="scriptdictionary">
        <item key="the lost bag"><![CDATA[
          PrintCentered ("\"Ha, here comes the man of justice. No, I'm not giving this to you unless you give me 20 coins. Besides, this whole bag only worth 10 coins.\"")
          Ask ("Give Johny 20 coins?") {
            if (result) {
              if (GetInt(GetObject("player"), "money") >= 20) {
                DecreaseMoney (20)
                msg ("For real, you need to change John. Use your time to do something good.")
                MoveObject (PickOneChild(GetObject("Thief")), player)
              }
              else {
                msg ("You don't have enough coins.")
              }
            }
          }
        ]]></item>
      </ask>
    </object>
